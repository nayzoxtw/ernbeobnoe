{
   "action": {
      "default_popup": "popup.html"
   },
   "author": "Samuel Hinchliffe",
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [
      {
         "js": ["jquery.js", "content.js"],
         "matches": ["https://www.chess.com/play/*", "https://www.chess.com/game/live/*"]
      }
   ],
   "description": "Improve your chess skills with Chess.com Analyzer - a free app to analyze your game using Stockfish to highlight the best move",
   "icons": {
      "128": "icon.png",
      "16": "icon.png",
      "32": "icon.png",
      "48": "icon.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Chess.com Analyzer",
   "permissions": ["activeTab", "storage"],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.3",
   "version_name": "First Release"
}
